{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        },
		{
			"key": "Access-Control-Allow-Origin",
			"value": "*"
		},
		{
			"key": "Access-Control-Allow-Credentials",
			"value": "true"
		}
      ]
    }
  ]
}
